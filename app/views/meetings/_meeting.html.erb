<div style="float:left; height: 350px; background: #f3f3f3; text-align:center; width: 165px;border:1px solid #999; padding:20px; margin:20px;">
	<h3>Date</h3>
	<%= meeting.start_at.to_s(:medium_day) %><br/>
	<%= meeting.start_at.to_s(:time) %> - 
	<%= meeting.end_at.to_s(:time) %>

	<br/><br/>
	<h3>Topic</h3>
	<%= meeting.description %>

	<br/><br/>
	<h3>Location</h3>
	<%= format_address_for_display(meeting.location) %>
	<br/>
</div>
